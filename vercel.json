{
    "version": 2,
    "builds": [
        {
            "src": "back/src/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "back/src/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}