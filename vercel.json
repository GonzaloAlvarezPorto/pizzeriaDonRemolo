{
    "version": 2,
    "builds": [
        {
            "src": "back/src/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/*",
            "use": "@vercel/static"
        } 
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "back/src/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        } 
    ]
}